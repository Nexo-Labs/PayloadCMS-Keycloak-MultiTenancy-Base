{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT",
        ".env*"
      ],
      "outputs": [
        ".turbo/**",
        ".next/**",
        ".vercel/**",
        "build/**",
        "dist/**"
      ]
    },
    "lint": {},
    "devcontainer": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": [
        "^build"
      ]
    },
    "storybook": {
      "cache": false,
      "persistent": true,
      "dependsOn": [],
      "outputs": []
    }
  }
}